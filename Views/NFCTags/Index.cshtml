@model IEnumerable<MiidWeb.NFCTag>

@{
	ViewBag.Title = "Index";



	Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
	//Layout = null;
}

<style>
	<!--

	.glyphicon {
		cursor: pointer;
	}

	input,
	select {
		width: 100%;
	}
	-->
</style>




<div class="col-md-9">
	<div class="row">
		<div class="col-md-12">
			<div class="content-box-header">
				<div class="panel-title">All Tickets</div>

				<div class="panel-options">

				</div>
			</div>
			<div class="content-box-large box-with-header">

				<div class="table-responsive" style="overflow-x:auto;">
					<table class="table table-striped table-bordered">

						<tr>
							<th colspan="3">Actions</th>
							<th>
								@Html.DisplayNameFor(model => model.TagNumber)
							</th>
							<th>
								@Html.DisplayNameFor(model => model.ActivationDate)
							</th>
							<th>
								@Html.DisplayNameFor(model => model.ActivationCode)
							</th>
							<th>
								@Html.DisplayNameFor(model => model.TagPin)
							</th>
							<th>
								@Html.DisplayNameFor(model => model.EndUser.Surname)
							</th>
							<th>
								@Html.DisplayNameFor(model => model.Status.Code)
							</th>

						</tr>

						@foreach (var item in Model)
						{
						<tr>
							<td>
								
								@Html.ActionLink("View", "Details", new { id = item.ID })
								
							</td>
							<td>
							
								@Html.ActionLink("Edit", "Edit", new { id = item.ID })
							
							</td>
							<td>
							
								@Html.ActionLink("Delete", "Delete", new { id = item.ID })
							</td>
							<td>
								@Html.DisplayFor(modelItem => item.TagNumber)
							</td>
							<td>
								@Html.DisplayFor(modelItem => item.ActivationDate)
							</td>
							<td>
								@Html.DisplayFor(modelItem => item.ActivationCode)
							</td>
							<td>
								@Html.DisplayFor(modelItem => item.TagPin)
							</td>
							<td>
								@Html.DisplayFor(modelItem => item.EndUser.Surname)
							</td>
							<td>
								@Html.DisplayFor(modelItem => item.Status.Code)
							</td>

						</tr>
						}

					</table>
					</div>
</div>
</div>
</div>
</div>
