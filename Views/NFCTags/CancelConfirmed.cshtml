@using Microsoft.AspNet.Identity
@model MiidWeb.Models.NFCTagViewModel
@{
    ViewBag.Title = "CancelByUser";

    var notification = ViewBag.Notification;
  //  MiidWeb.Controllers.UserModel u1 = MiidWeb.Helpers.SessionManager.EndUser;
}

<div class="container bg-grey">
	<div class="row vertical-offset-40px">


		<div class="col-md-6 col-md-offset-3">

			<div class="panel panel-default ">

				<div class="panel-body text-center">

					<br>


					@using (Html.BeginForm())
					{
						@Html.AntiForgeryToken()
						<!-- One -->

						@Html.HiddenFor(x => x.IDNumber)
						@Html.HiddenFor(x => x.ID)
						@Html.HiddenFor(x => x.NewIDNumber)


						<h1 class="fa fa-exclamation-triangle  fa-5x" style="color:#be1c1d"></h1>

						<h3>
							Are you sure you want to cancel your band? <span>@Model.TagNumber</span>
						</h3>

						<br>


						<input type="submit" class="btn btn-default btn-block btn-lg " value="Yes" />

						<a href="~/NFCTags/IndexByUserID" class="btn btn-info btn-block btn-lg">No</a>



					}

				</div>
			</div>
		
		</div>
	</div>
</div>
     
