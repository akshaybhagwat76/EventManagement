@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.Hidden("EventID", (object)@ViewBag.EventID)



    @Html.HiddenFor(model => model.TicketClass.ID);

    <!--main content start-->
    
            <!-- //market-->


            <div class="row">
                <div class="panel-body">
					<div class="col-md-12 w3ls-graph">
						<!--agileinfo-grap-->
						<div class="agileinfo-grap">
							<div class="agileits-box">
								<form method="post" action="#">
									<div class="agileits-box-body clearfix">

										<div class="form-group col-md-12">
											<h2>Create multiple tickets</h2>
											<h3>Ticket Details:</h3>
											<p>
												<div>
													Event Start: @Model.Event.StartDateTime.Value.ToString("yyyy-MM-dd HH:mm")
													End: @Model.Event.EndDateTime.Value.ToString("yyyy-MM-dd HH:mm")
												</div>
											</p>
											<p><b><br>Tip:</b> Be descriptive when creating your ticket name so you can easily remember it when viewing reports.</p>

										</div>
										<div class="form-group col-md-3">
											<label for="exampleInputEmail1">Display Name</label>
											@Html.TextBoxFor(model => model.TicketClassDescription, new { @class = "form-control input-lg valid", placeholder = "Ticket Name. eg , Vip, Early Bird, General" })

                                        <small id="emailHelp" class="form-text text-muted">This name that will appear on the website. Date added automatically</small>
										</div>
										<div class="form-group col-md-3">
											<label for="exampleInputEmail1">Code</label>
											@Html.TextBoxFor(model => model.TicketClass.Code, new { @class = "form-control input-lg valid", placeholder = "e.g. General Access or VIP" })
                                        <small id="emailHelp" class="form-text text-muted">This will appear on your dashboard and reports. Date added automatically</small>
										</div>

										<div class="form-group col-md-3">
											<label for="exampleInputEmail1">Available Quantity</label>
											@Html.TextBoxFor(model => model.TicketClass.Quantity, new { @class = "form-control input-lg valid", placeholder = "0", type = "number" })
											@Html.ValidationMessageFor(model => model.TicketClass.Quantity, "", new { @class = "red" })
											<small id="emailHelp" class="form-text text-muted">The max amount of tickets available</small>
										</div>
										<div class="form-group col-md-3">
											<label for="exampleInputEmail1">Rand Value</label>

											@Html.TextBoxFor(model => model.TicketClass.Price, new { @class = "form-control input-lg valid", placeholder = "120", required = "required", type = "number" })
											@Html.ValidationMessageFor(model => model.TicketClass.Price, "", new { @class = "red" })
											<small id="emailHelp" class="form-text text-muted">The selling price of the ticket</small>
										</div>
										
										
										<div class="form-group col-md-12">
											<hr>
											<label>Generate individual ticket type for multiple days: click the textbox to select multiple dates</label>
											<input type="text" class="form-control input-lg date" placeholder="Pick the multiple dates" id="MultipleDates" name="MultipleDates" autocomplete="off" onfocus="blur();">
											<hr>
										</div>


									</div>

                                    <div class="agileits-box-body clearfix">
                                        <div class="alert alert-info">
                                            <div class="row">
                                                <div class="form-group col-md-12 ">
                                                    <h3>Valid from:</h3>
                                                    <p>
                                                        Only change if event is over multiple days or time slots. By changing this you will change the time when the ticket can be scanned with the Mi-id app.
                                                    </p>
                                                </div>
                                                <div class="form-group col-md-12">
                                                    <p>
                                                        <strong>Start time cannot before  @Model.Event.StartDateTime.Value.ToString("yyyy-MM-dd HH:mm") and after  @Model.Event.EndDateTime.Value.ToString("yyyy-MM-dd HH:mm")</strong>

                                                        @Html.ValidationSummary(true, "", new { @class = "red" })


                                                    </p>
                                                </div>

                                                <div class="form-group col-md-12">
                                                    @Html.ValidationMessage("EndDateTime", new { style = "color: red" })

                                                </div>
                                                <div class="form-group col-md-3">
                                                    <label for="exampleInputEmail1">Start Time</label>
                                                    @Html.DropDownListFor(x => x.StartDateTimeTime, (IEnumerable<SelectListItem>)Model.StartDateTimeTimes, new { @class = "form-control input-lg" })

                                                </div>
                                                <div class="form-group col-md-3">
                                                    <label for="exampleInputEmail1">End Time</label>
                                                    @Html.DropDownListFor(x => x.EndDateTimeTime, (IEnumerable<SelectListItem>)Model.EndDateTimeTimes, htmlAttributes: new { @class = "form-control input-lg" })

                                                   
